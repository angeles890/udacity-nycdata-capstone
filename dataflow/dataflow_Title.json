{
	"name": "dataflow_Title",
	"properties": {
		"folder": {
			"name": "DataLakeToSynapse"
		},
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "ds_TitleMaster",
						"type": "DatasetReference"
					},
					"name": "CSVTitle"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "ds_synapse_NYC_Payroll_TITLE_MD",
						"type": "DatasetReference"
					},
					"name": "SinkTitle"
				}
			],
			"transformations": [],
			"scriptLines": [
				"source(output(",
				"          TitleCode as string,",
				"          TitleDescription as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> CSVTitle",
				"CSVTitle sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     input(",
				"          TitleCode as string,",
				"          TitleDescription as string",
				"     ),",
				"     deletable:false,",
				"     insertable:true,",
				"     updateable:false,",
				"     upsertable:false,",
				"     format: 'table',",
				"     staged: true,",
				"     allowCopyCommand: true,",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     errorHandlingOption: 'stopOnFirstError') ~> SinkTitle"
			]
		}
	}
}